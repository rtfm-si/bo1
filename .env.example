# LLM API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
VOYAGE_API_KEY=your_voyage_api_key_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Development Settings
# DEBUG: When true, shows all bo1 module logs. When false, suppresses internal bo1 logs.
DEBUG=true
# LOG_LEVEL: Controls log verbosity (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# Use INFO or DEBUG to see detailed logs, WARNING for cleaner output
LOG_LEVEL=INFO

# Cost Limits (v1)
MAX_COST_PER_SESSION=1.00  # USD

# A/B Testing
AB_TESTING_ENABLED=true

# v2 Settings (not used in v1, documented for future)
# DATABASE_URL=postgresql://bo1:password@localhost:5432/bo1
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_KEY=your_supabase_key
# SUPABASE_ANON_KEY=your_supabase_anon_key
# POSTGRES_PASSWORD=your_postgres_password
# GRAFANA_PASSWORD=your_grafana_password
