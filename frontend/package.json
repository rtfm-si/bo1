{
  "name": "board-of-one-frontend",
  "version": "1.0.0",
  "description": "Board of One Web UI - Multi-agent deliberation system",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "postinstall": "node scripts/patch-frappe-gantt.js",
    "generate:types": "openapi-typescript ../openapi.json -o src/lib/api/generated-types.ts && node scripts/save-openapi-hash.js",
    "check:types-fresh": "node scripts/check-types-fresh.js"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.562.0",
    "@playwright/test": "1.57.0",
    "@sveltejs/adapter-node": "5.4.0",
    "@sveltejs/kit": "2.49.2",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tailwindcss/cli": "4.1.18",
    "@tailwindcss/vite": "4.1.18",
    "@types/dompurify": "3.2.0",
    "@types/js-cookie": "3.0.6",
    "@types/node": "24.10.1",
    "@vitest/ui": "4.0.16",
    "clsx": "^2.1.1",
    "openapi-typescript": "^7.10.1",
    "svelte": "5.46.0",
    "svelte-check": "4.3.5",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  },
  "dependencies": {
    "@types/marked": "6.0.0",
    "driver.js": "^1.4.0",
    "frappe-gantt": "1.0.4",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "2.34.0",
    "js-cookie": "3.0.5",
    "lucide-svelte": "0.562.0",
    "marked": "17.0.1",
    "supertokens-web-js": "0.16.0",
    "svelte-dnd-action": "0.9.68"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.54.0",
    "@tailwindcss/oxide-linux-arm64-gnu": "4.1.18",
    "@tailwindcss/oxide-linux-x64-gnu": "4.1.18",
    "lightningcss-linux-arm64-gnu": "1.30.2",
    "lightningcss-linux-x64-gnu": "1.30.2"
  }
}
