{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "row",
      "title": "Session Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Active Sessions",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "bo1_active_sessions or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "orange", "value": 10 }, { "color": "red", "value": 20 }] } } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Sessions Started (24h)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "sum(increase(bo1_sessions_total{status=\"started\"}[24h])) or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Sessions Completed (24h)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "sum(increase(bo1_sessions_total{status=\"completed\"}[24h])) or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short" } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "gauge",
      "title": "Completion Rate (24h)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "(sum(increase(bo1_sessions_total{status=\"completed\"}[24h])) / sum(increase(bo1_sessions_total{status=\"started\"}[24h]))) * 100 or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "percent", "min": 0, "max": 100, "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "orange", "value": 80 }, { "color": "green", "value": 95 }] } } },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "row",
      "title": "Session Metrics",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Sessions Over Time",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "sum(rate(bo1_sessions_total{status=\"started\"}[1h])) * 3600", "legendFormat": "Started", "refId": "A" },
        { "expr": "sum(rate(bo1_sessions_total{status=\"completed\"}[1h])) * 3600", "legendFormat": "Completed", "refId": "B" },
        { "expr": "sum(rate(bo1_sessions_total{status=\"failed\"}[1h])) * 3600", "legendFormat": "Failed", "refId": "C" }
      ],
      "fieldConfig": { "defaults": { "unit": "short", "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 } } }
    },
    {
      "type": "timeseries",
      "title": "Deliberation Rounds",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "sum(rate(bo1_deliberation_rounds_total[5m])) * 300", "legendFormat": "Rounds/5m", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 } } }
    },
    {
      "type": "row",
      "title": "Performance",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Active Sessions Over Time",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [{ "expr": "bo1_active_sessions or vector(0)", "legendFormat": "Active", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20 } } }
    },
    {
      "type": "timeseries",
      "title": "Session Duration Distribution",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(bo1_session_duration_seconds_bucket[1h])) by (le))", "legendFormat": "P50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum(rate(bo1_session_duration_seconds_bucket[1h])) by (le))", "legendFormat": "P95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(bo1_session_duration_seconds_bucket[1h])) by (le))", "legendFormat": "P99", "refId": "C" }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "drawStyle": "line", "lineWidth": 2 } } }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["deliberation", "sessions"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Deliberation Dashboard",
  "uid": "bo1-deliberation",
  "version": 1
}
